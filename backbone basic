        var MyClass = Backbone.Model.extend({
            defaults:{
                name:"tharun",
                section:"12th",
                grade:"first"
            },
            initialize:function(){
                document.write("</br><h1>"+"this is collection of extend : "+"</h1>")
            }
        });
        var MyClass1 = Backbone.Collection.extend({
            model:MyClass
        });

        var student = new MyClass({
            name:"saran",
            section:"12th",
            grade:"second"
        });
        var myval1 = new MyClass1([student]);
        document.write(JSON.stringify(myval1.models));   
//2
        var ships= new Backbone.Collection;
        ships.on("add",function(ship){
            document.write("</br>"+"dutchell" + ship.get("name"));
        })
        ships.add([
            {name:"flying Man"},
            {name:"Blackpearl"}
        ]);
