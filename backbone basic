        var Route = Backbone.View.extend({
            el:'#menu',
            events:{
                'click a':'onClick',
                'click #btn':'tex'
            },
            onClick:function(e){
                router.navigate('/')
            },
            tex:function(){
                var a=10;
                var b=20;
                var c=(a+b);
                document.getElementById('menu').innerHTML=c;
            }
        })
